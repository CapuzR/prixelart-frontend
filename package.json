    "aws-sdk": "^2.886.0",
    "axios": "^1.6.5",
    "dayjs": "^1.11.13",
    "exceljs": "^4.3.0",
    "immutable": "^4.1.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.43",
    "mongodb": "^6.12.0",
    "nanoid": "^3.1.23",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-cool-img": "^1.2.9",
    "react-dom": "^18.3.1",
    "react-easy-crop": "^3.3.3",
    "react-ga": "^3.3.1",
    "react-hook-form": "^7.53.2",
    "react-quill": "^2.0.0",
    "react-responsive-masonry": "^2.1.6",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.29.0",
    "recharts": "^2.8.0",
    "sass": "^1.80.6",
    "slick-carousel": "^1.8.1",
    "victory": "^36.6.11",
    "vite": "^5.4.11"
  },
  "scripts": {
    "dev": "yarn vite",
    "build": "yarn vite build",
    "serve": "yarn vite preview",
    "prepare": "yarn husky install",
    "format": "yarn prettier --config config/.prettierrc --ignore-path config/.prettierignore --write .",
    "lint": "yarn eslint -c config/eslint.config.js --ignore-pattern config/.eslintignore ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged --ignore-pattern \"**/*.test.{js,jsx,ts,tsx}\""
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --config config/.prettierrc --write",
      "eslint -c config/.eslintrc --fix"
    ],
    "*.test.{js,jsx,ts,tsx}": []
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-unicorn": "^56.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.3"
  }
}
