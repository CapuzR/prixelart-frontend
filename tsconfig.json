{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "apps/admin/src/*",
        "apps/consumer/src/*"
      ],
      "@components/*": [
        "apps/admin/src/components/*",
        "apps/consumer/src/components/*",
        "apps/consumer/components/*"
      ],
      "components/*": [
        "apps/admin/src/components/*",
        "apps/consumer/src/components/*",
        "apps/consumer/components/*"
      ],
      "@utils/*": [
        "apps/admin/src/utils/*",
        "apps/consumer/src/utils/*"
      ],
      "utils/*": [
        "apps/admin/src/utils/*",
        "apps/consumer/src/utils/*"
      ],
      "@styles/*": [
        "apps/admin/styles/*",
        "apps/consumer/styles/*"
      ],
      "styles/*": [
        "apps/admin/styles/*",
        "apps/consumer/styles/*"
      ],
      "@assets/*": [
        "apps/admin/assets/*",
        "apps/consumer/assets/*"
      ],
      "assets/*": [
        "apps/admin/assets/*",
        "apps/consumer/assets/*"
      ],
      "@public/*": [
        "apps/admin/public/*",
        "apps/consumer/public/*",
        "public/*"
      ],
      "@apps/admin/*": ["apps/admin/src/*"],
      "apps/admin/*": ["apps/admin/src/*"],
      "@apps/artist/*": ["apps/consumer/src/artist/*", "apps/artist/src/*"],
      "apps/artist/*": ["apps/consumer/src/artist/*", "apps/artist/src/*"],
      "@apps/consumer/*": ["apps/consumer/src/*"],
      "apps/consumer/*": ["apps/consumer/src/*"],
      "@apps/map/*": ["apps/map/src/*"],
      "@apps/orgs/*": ["apps/orgs/src/*"],
      "@data/*": ["apps/consumer/src/data/*"],
      "@context/*": ["packages/context/src/*"],
      "context/*": ["apps/consumer/src/context/*", "packages/context/src/*"],
      "@prixpon/design-system": ["packages/design-system/src"],
      "@prixpon/design-system/*": ["packages/design-system/src/*"],
      "@prixpon/types": ["packages/types/src"],
      "@prixpon/types/*": ["packages/types/src/*"],
      "@prixpon/api-client": ["packages/api-client/src"],
      "@prixpon/api-client/*": ["packages/api-client/src/*"],
      "@prixpon/tokens": ["packages/tokens/src"],
      "@prixpon/tokens/*": ["packages/tokens/src/*"],
      "@prixpon/config": ["packages/config/src"],
      "@prixpon/config/*": ["packages/config/src/*"],
      "@prixpon/utils": ["packages/utils/src"],
      "@prixpon/utils/*": ["packages/utils/src/*"],
      "@prixpon/context": ["packages/context/src"],
      "@prixpon/context/*": ["packages/context/src/*"],
      "@prixpon/data": ["packages/data/src"],
      "@prixpon/data/*": ["packages/data/src/*"]
    },
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "module": "esnext",
    "target": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "outDir": "./dist",
    "typeRoots": ["./node_modules/@types"],
    "rootDir": ".",
    "types": ["node", "react"]
  },
  "include": ["apps", "packages"],
  "exclude": ["node_modules", "dist"]
}