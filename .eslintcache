eRow>\n        </TableHead>\n        <TableBody>\n        {\n        rows &&\n          rows.map((row) => (\n            <TableRow key={row._id}>\n              <TableCell align=\"center\">\n                <Fab color=\"default\" style={{width: 35, height: 35}} aria-label=\"edit\" onClick={(e)=>{handleActive(row, 'update')}}>\n                  <EditIcon/>\n                </Fab>\n              </TableCell>\n              <TableCell align=\"center\"><img alt=\"thumb\" src={row.thumbUrl} style={{width: 50, height: 'auto'}}/></TableCell>\n              <TableCell align=\"center\">{row.name}</TableCell>\n              <TableCell align=\"center\">\n                <Checkbox \n                  disabled\n                  checked={row.active}\n                  color=\"primary\" \n                  inputProps={{ 'aria-label': 'secondary checkbox' }}\n                />\n              </TableCell>\n              <TableCell align=\"center\">{row.category}</TableCell>\n              <TableCell align=\"center\">{row.publicPrice.from}-{row.publicPrice.to}</TableCell>\n              <TableCell align=\"center\">{row.prixerPrice.from}-{row.prixerPrice.to}</TableCell>\n            </TableRow>\n          ))\n        }\n        </TableBody>\n      </Table>\n} \n    </React.Fragment>\n  );\n}","/home/capuzr/Documentos/Prixerapp/frontend/src/admin/adminMain/products/products.jsx",[],"/home/capuzr/Documentos/Prixerapp/frontend/src/admin/productCrud/createProduct.jsx",[],"/home/capuzr/Documentos/Prixerapp/frontend/src/admin/productCrud/updateProduct.jsx",["399"],"/home/capuzr/Documentos/Prixerapp/frontend/src/admin/productCrud/disableProduct.jsx",[],"/home/capuzr/Documentos/Prixerapp/frontend/src/admin/adminMain/products/variants.jsx",[],"/home/capuzr/Documentos/Prixerapp/frontend/src/admin/productCrud/variants/createVariant.jsx",["400","401","402","403","404","405","406","407","408","409","410","411","412","413","414","415","416","417","418","419","420","421","422","423","424","425","