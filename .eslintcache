ngth === 5 ?\n      setActiveCrud(location.pathname.split('/')[location.pathname.split('/').length-2])\n    : location.pathname.split('/').length === 4 &&\n      setActiveCrud(location.pathname.split('/')[location.pathname.split('/').length-1])\n  },[location.pathname]);\n\n  return (\n    <div style={{position:'relative'}}>\n      {\n        productEdit &&\n        <div style={{position:'absolute', right:0}}>\n          <Fab color=\"default\" aria-label=\"edit\" onClick={()=>{handleProductAction('read')}}>\n            <ViewListIcon />\n          </Fab>\n          {/* <Fab color=\"secondary\" aria-label=\"edit\" onClick={()=>{handleUserAction('update')}}>\n            <EditIcon />\n          </Fab> */}\n          <Fab color=\"primary\" aria-label=\"add\" onClick={()=>{handleProductAction('create')}}>\n            <AddIcon />\n          </Fab>\n        </div>\n      }\n      <Grid container spacing={3} style={{margin: isDesktop ? '12px' : ''}}>\n          {/* Chart */}\n          <Grid item xs={12} md={12} lg={12}>\n              <Paper className={fixedHeightPaper} >\n                {\n                  activeCrud === 'create' ?\n                    <CreateProduct />\n                  : activeCrud === 'read' ?\n                    <ReadProducts setProduct={setProduct}/>\n                  : activeCrud === 'update' ?\n                    <div style={{height: '115vh'}}>\n                      <UpdateProduct product={product} setProductEdit={setProductEdit}/>\n                    </div>\n                  :\n                    <DisableProduct />\n                }\n              </Paper>\n          </Grid>\n      </Grid>\n    </div>\n  );\n}\n","/home/tomas/Escritorio/Prixelart_web/prixelart-frontend/src/admin/adminMain/dashboard/dashboard.jsx",[],"/home/tomas/Escritorio/Prixelart_web/prixelart-frontend/src/admin/adminMain/adminUser/adminUser.jsx",[],"/home/tomas/Escritorio/Prixelart_web/prixelart-frontend/src/products/services.js",["401","402","403","404","405","40